{
  "name": "audio2video",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "An Electron application for converting audio to video",
  "dependencies": {
    "fluent-ffmpeg": "^2.1.3",
    "music-metadata": "^11.7.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "build": {
    "appId": "com.example.audio2video",
    "productName": "audio2video",
    "files": [
      "dist/",
      "index.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "category": "AudioVideo"
    },
    "publish": {
      "provider": "github",
      "private": false
    }
  }
}
